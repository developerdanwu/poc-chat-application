{
  "display_information": {
    "name": "Chat GPT app"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": false,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "Chat GPT app",
      "always_online": false
    },
    "slash_commands": [
      {
        "command": "/summary",
        "url": "https://8f3c-185-174-108-6.ngrok-free.app/api/slack/conversation-summary",
        "description": "fetch conversation summary",
        "should_escape": false
      },
      {
        "command": "/imagine",
        "url": "https://8f3c-185-174-108-6.ngrok-free.app/api/slack/imagine",
        "description": "Generates an image",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "user": [
        "im:history"
      ],
      "bot": [
        "app_mentions:read",
        "channels:read",
        "chat:write",
        "chat:write.public",
        "commands",
        "files:write",
        "im:history",
        "files:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "https://8f3c-185-174-108-6.ngrok-free.app/api/slack/events",
      "user_events": [
        "message.im"
      ],
      "bot_events": [
        "app_home_opened",
        "app_mention",
        "channel_created",
        "message.im"
      ]
    },
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://8f3c-185-174-108-6.ngrok-free.app/api/slack/interactive"
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}